<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="andycui.top.uicontrols">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AtyUsingRadioGroup" />
        <activity android:name=".AtyUsingCheckBox"></activity>
        <activity android:name=".AtyUsingDataPicker"></activity>
        <activity android:name=".AtyUsingTimePicker"></activity>
        <activity android:name=".AtyUsingSpinner"></activity>
        <activity android:name=".AtyUseingProgressBar"></activity>
        <activity android:name=".AtyUsingAutoCompleteTextView"></activity>
        <activity android:name=".AtyUsingSeekBar"></activity>
        <activity android:name=".AtyUsingCridView"></activity>
        <activity android:name=".AtyUsingProgressDialog"></activity>
        <activity android:name=".AtyUsingNotification"></activity>
    </application>

</manifest>